/* pages/myCoffee/page/myDevice/bindingDevice/bindingDevice.wxss */
page{ background: #f7f7f7;}
.add-img{ width: 234rpx; display: block; margin: 0 auto }
.box{ padding: 100rpx  50rpx;}
.form{  width: 450rpx; margin: 0 auto; padding-top: 100rpx;}
.btn{
	width: 100%;
	line-height: 60rpx;
	background-image: linear-gradient(-75deg, 
		#2d2d2d 0%, 
		#111111 100%), 
	linear-gradient(
		#ffffff, 
		#ffffff);
	background-blend-mode: normal, 
		normal;
	border-radius: 30rpx;
	color: #fff;
	 text-align: center;
	margin: 100rpx auto;
}
.txt{
	width: 100%;
	height: 60rpx;
	padding: 10rpx 20rpx; box-sizing: border-box;
	background-color: #ffffff;
	border: solid 1rpx #e6e6e6;
	 margin-bottom: 30rpx;
}